{
  "name": "lkd-web-kit",
  "version": "0.0.0",
  "description": "A template for creating React component libraries with Vite.",
  "author": "LKD",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./components": {
      "import": {
        "types": "./dist/components/index.d.ts",
        "default": "./dist/components/index.js"
      },
      "require": {
        "types": "./dist/components/index.d.ts",
        "default": "./dist/components/index.cjs"
      }
    },
    "./consts": {
      "import": {
        "types": "./dist/consts/index.d.ts",
        "default": "./dist/consts/index.js"
      },
      "require": {
        "types": "./dist/consts/index.d.ts",
        "default": "./dist/consts/index.cjs"
      }
    },
    "./contexts" :{
      "import": {
        "types": "./dist/contexts/index.d.ts",
        "default": "./dist/contexts/index.js"
      },
      "require": {
        "types": "./dist/contexts/index.d.ts",
        "default": "./dist/contexts/index.cjs"
      }
    },
    "./form": {
      "import": {
        "types": "./dist/form/index.d.ts",
        "default": "./dist/form/index.js"
      },
      "require": {
        "types": "./dist/form/index.d.ts",
        "default": "./dist/form/index.cjs"
      }
    },
    "./hocs": {
      "import": {
        "types": "./dist/hocs/index.d.ts",
        "default": "./dist/hocs/index.js"
      },
      "require": {
        "types": "./dist/hocs/index.d.ts",
        "default": "./dist/hocs/index.cjs"
      }
    },

    "./hooks": {
      "import": {
        "types": "./dist/hooks/index.d.ts",
        "default": "./dist/hooks/index.js"
      },
      "require": {
        "types": "./dist/hooks/index.d.ts",
        "default": "./dist/hooks/index.cjs"
      }
    },
    "./mantine":{
      "import": {
        "types": "./dist/mantine/index.d.ts",
        "default": "./dist/mantine/index.js"
      },
      "require": {
        "types": "./dist/mantine/index.d.ts",
        "default": "./dist/mantine/index.cjs"
      }
    },
    "./utils": {
      "import": {
        "types": "./dist/utils/index.d.ts",
        "default": "./dist/utils/index.js"
      },
      "require": {
        "types": "./dist/utils/index.d.ts",
        "default": "./dist/utils/index.cjs"
      }
    },
    "./types": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/types/index.js"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/types/index.cjs"
      }
    }
  },
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier . --write --ignore-unknown"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.2",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "@vitejs/plugin-react-swc": "^3.8.1",
    "@vitest/coverage-v8": "^3.1.1",
    "clsx": "^2.1.1",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "jsdom": "^26.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "vite": "^6.2.5",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^3.1.1"
  },
  "peerDependencies": {
    "@mantine/core": "^7.17.4",
    "@mantine/hooks": "^7.17.4",
    "@tanstack/react-query": "^5.72.2",
    "ky": "^1.8.0",
    "next": "^15.3.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.55.0",
    "react-query-kit": "^3.3.1",
    "zod": "^3.24.2"
  }
}
